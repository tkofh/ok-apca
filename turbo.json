{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		},
		"generate": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".output/**"]
		},
		"preview": {
			"dependsOn": ["generate"],
			"cache": false,
			"persistent": true,
			"interruptible": true
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"test": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true,
			"interruptible": true
		},
		"//#check": {
			"inputs": ["$TURBO_DEFAULT$", "biome.json"]
		},
		"//#check:force": {
			"inputs": ["$TURBO_DEFAULT$", "biome.json"]
		},
		"//#check:report": {
			"inputs": ["$TURBO_DEFAULT$", "biome.json"],
			"cache": false
		},
		"//#format": {
			"inputs": ["$TURBO_DEFAULT$", "biome.json"]
		}
	}
}
